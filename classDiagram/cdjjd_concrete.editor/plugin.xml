<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated concrete -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="concrete.presentation.ConcreteModelWizardID"
            name="%_UI_ConcreteModelWizard_label"
            class="concrete.presentation.ConcreteModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ConcreteModelFile.gif">
         <description>%_UI_ConcreteModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated concrete -->
      <editor
            id="concrete.presentation.ConcreteEditorID"
            name="%_UI_ConcreteEditor_label"
            icon="icons/full/obj16/ConcreteModelFile.gif"
            extensions="concrete"
            class="concrete.presentation.ConcreteEditor"
            contributorClass="concrete.presentation.ConcreteActionBarContributor">
      </editor>
   </extension>

</plugin>
