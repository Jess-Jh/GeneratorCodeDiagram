		/**
 * @generated
 */
public static int getLinkWithClassVisualID(org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	if (concrete.ConcretePackage.eINSTANCE.getAgregationJJD().isSuperTypeOf(domainElement.eClass())) {
	return concrete.diagram.edit.parts.AgregationJJDEditPart.VISUAL_ID;
}
if (concrete.ConcretePackage.eINSTANCE.getInheritanceJJD().isSuperTypeOf(domainElement.eClass())) {
	return concrete.diagram.edit.parts.InheritanceJJDEditPart.VISUAL_ID;
}
if (concrete.ConcretePackage.eINSTANCE.getContainmentJJD().isSuperTypeOf(domainElement.eClass())) {
	return concrete.diagram.edit.parts.ContainmentJJDEditPart.VISUAL_ID;
}
if (concrete.ConcretePackage.eINSTANCE.getAssociationJJD().isSuperTypeOf(domainElement.eClass())) {
	return concrete.diagram.edit.parts.AssociationJJDEditPart.VISUAL_ID;
}
if (concrete.ConcretePackage.eINSTANCE.getImplementJJD().isSuperTypeOf(domainElement.eClass())) {
	return concrete.diagram.edit.parts.ImplementJJDEditPart.VISUAL_ID;
}
	return -1;
}
	
